{
  "name": "pinia-plugin-keep",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.5.1",
  "description": "üçç A pinia persistent plugin",
  "author": {
    "email": "lne128gene980@163.com",
    "name": "JiatLn"
  },
  "license": "MIT",
  "homepage": "https://github.com/jiatln/pinia-plugin-keep#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiatln/pinia-plugin-keep.git"
  },
  "bugs": "https://github.com/jiatln/pinia-plugin-keep/issues",
  "keywords": [
    "pinia",
    "plugin",
    "keep",
    "persistent"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "vite playground --open",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "pinia": "^2.0.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.0.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.18.0"
  }
}
